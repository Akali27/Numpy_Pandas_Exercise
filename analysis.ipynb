    "#Most Profitable Items\n",
    "left_frame = purchase_data_df[['Item ID', 'Item Name', 'Price']]\n",
    "right_frame = purchase_data_df[['Item ID', 'Price']].groupby('Item ID').agg(['sum', 'count']).reset_index()\n",
